@model HomeViewModel

<partial name="_Carousel" />

<section class="text-center mt-5">
    <h1>Properties of the week</h1>
    <h5>Modern Elegance Meets Timeless Comfort: Your Dream Home Awaits</h5>

    <div class="row properties-of-the-week">
        <div class="row">
            @foreach (var property in Model.PropertiesOfTheWeek)
            {
                <partial name="_PropertyCard" model="property" />
            }
        </div>
    </div>
</section>
